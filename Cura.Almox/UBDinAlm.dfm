  object Q_OC: TwwQuery
    DatabaseName = 'CURA'
    SessionName = 'Sec_BaseFin'
    DataSource = DS_ALM
    SQL.Strings = (
      'SELECT'#9#9'SUM(OI.QTDE_COMP - '
      #9#9'CASE WHEN OI.QTDE_COMP_REC IS NULL THEN 0'
      #9#9'ELSE OI.QTDE_COMP_REC              END) QTDE,'
      #9#9'OI.ORD_ID,'
      #9#9'OI.UNI_COMP_ID,'
      #9#9'OC.DT_ENTREGA,'
      #9#9'FORN.FOR_RAZA'
      'FROM'#9#9'ORDEM_COMPRA OC,'
      #9#9'ORDEM_COMPRAITEM  OI,'
      #9#9'CPAFORN FORN'
      'WHERE'#9#9'OI.MAT_ID =:MAT_ID'
      'AND'#9#9'OI.ORD_ID = OC.ORD_ID'
      'AND'#9#9'OC.FOR_CODI = FORN.FOR_CODI'
      'AND'#9#9'OC.STATUS IN ('#39'A'#39','#39'P'#39')'
      'AND'#9#9'OI.STATUS  IN ('#39'A'#39','#39'P'#39')'
      
        'AND'#9#9'(OI.QTDE_COMP - QTDE_COMP_REC > 0.001  OR QTDE_COMP_REC IS ' +
        ' NULL )'
      'AND             OC.STATUS <>  '#39'F'#39' '
      'AND'#9'        OI.STATUS  <>  '#39'T'#39' '
      'AND            OI.STATUS <> '#39'C'#39
      'GROUP BY'#9'OI.ORD_ID,'
      #9#9'OI.UNI_COMP_ID,'
      #9#9'OC.DT_ENTREGA,'
      #9#9'FORN.FOR_RAZA')
    ValidateWithMask = True
    Left = 374
    Top = 218
