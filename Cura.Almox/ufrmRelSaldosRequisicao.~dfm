  object qrBusca: TADOQuery
    Connection = FMenu.ADOConnection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT    TOP 1000'
      #9'S.SOL_ID,'
      #9'S.DATA,'
      #9'S.USUARIO,'
      #9'M.MAT_DESC,'
      #9'I.QTDE_SOL,'
      #9'I.SALDO,'
      #9'I.STATUS ,'
      #9'CC.CC_Desc'
      'FROM SolMaterial S LEFT OUTER JOIN ITEM_SOLMATERIAL I'
      'ON S.SOL_ID = I.SOL_ID LEFT OUTER JOIN MATERIAIS M'
      'ON I.MAT_ID = M.MAT_ID INNER JOIN CENTRO_CUSTO CC'
      'ON S.CC_ID = CC.CC_ID')
    Left = 337
    Top = 95
    object qrBuscaSOL_ID: TAutoIncField
      DisplayLabel = 'Código'
      DisplayWidth = 6
      FieldName = 'SOL_ID'
      ReadOnly = True
    end
    object qrBuscaDATA: TDateTimeField
      DisplayLabel = 'Data'
      DisplayWidth = 8
      FieldName = 'DATA'
      DisplayFormat = 'DD/MM/YY'
    end
    object qrBuscaMAT_DESC: TStringField
      DisplayLabel = 'Material'
      DisplayWidth = 35
      FieldName = 'MAT_DESC'
      Size = 60
    end
    object qrBuscaQTDE_SOL: TFloatField
      DisplayLabel = 'Qtde'
      DisplayWidth = 7
      FieldName = 'QTDE_SOL'
    end
    object qrBuscaSALDO: TFloatField
      DisplayLabel = 'Saldo'
      DisplayWidth = 7
      FieldName = 'SALDO'
    end
    object qrBuscaCC_Desc: TStringField
      DisplayLabel = 'Centro de Custo'
      DisplayWidth = 20
      FieldName = 'CC_Desc'
      Size = 40
    end
    object qrBuscaUSUARIO: TStringField
      DisplayLabel = 'Usuário'
      DisplayWidth = 30
      FieldName = 'USUARIO'
      Size = 50
    end
    object qrBuscaSTATUS: TStringField
      DisplayWidth = 1
      FieldName = 'STATUS'
      Visible = False
      FixedChar = True
      Size = 1
    end
  end
