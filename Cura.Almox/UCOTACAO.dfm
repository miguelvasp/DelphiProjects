  object qrMateriais: TADOQuery
    Connection = FMenu.ADOConnection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT M.MAT_ID, CodigoMaterial + '#39' '#39' + MAT_DESC AS MAT_DESC'
      'FROM MATERIAIS M'
      'INNER JOIN MATERIAL_MARCA MM ON M.MAT_ID = MM.MAT_ID'
      'WHERE  (GENERICO is null or GENERICO = '#39'N'#39')'
      'AND MAT_ATIVO = '#39'S'#39
      'AND ISNULL(MATMARCA_CONVERSAO, 0) > 0'
      'ORDER BY MAT_DESC')
    Left = 360
    Top = 464
