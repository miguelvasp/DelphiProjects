  object qrVerificaEstoque: TADOQuery
    Connection = FMenu.ADOConnection
    Parameters = <
      item
        Name = 'local'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end
      item
        Name = 'mat_id'
        Attributes = [paSigned]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end
      item
        Name = 'lote'
        DataType = ftFixedChar
        NumericScale = 255
        Precision = 255
        Size = 20
        Value = Null
      end>
    SQL.Strings = (
      #9#9'SELECT '
      
        #9#9'SUM(CASE WHEN ALM.QTDE IS NULL THEN 0.0 ELSE ALM.QTDE END) as ' +
        'Qtde'
      
        #9#9'FROM ALMOX ALM, MATERIAIS'#9'MAT INNER JOIN GRUPO_MATERIAL G ON M' +
        'AT.GRU_ID = G.GRU_ID, LOCAL LOC   '
      
        #9#9'WHERE ALM.MAT_ID = MAT.MAT_ID  AND'#9#9'ALM.LOCAL_ID'#9'= LOC.LOCAL_I' +
        'D  '
      #9#9'AND'#9'ALM.TIPO = '#39'E'#39' AND'#9#9'ALM.QTDE_RETIRADA IS NULL  '
      #9#9'AND LOC.LOCAL_ID = :local'
      #9#9'and mat.mat_id = :mat_id'
      #9#9'and alm.n_lote = :lote')
    Left = 225
    Top = 439
    object qrVerificaEstoqueQtde: TFloatField
      FieldName = 'Qtde'
      ReadOnly = True
    end
  end
