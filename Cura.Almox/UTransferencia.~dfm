  object qrMateriais: TADOQuery
    Connection = FMenu.ADOConnection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT MAT_ID, MAT_DESC FROM MATERIAIS'
      'WHERE GENERICO NOT IN ('#39'S'#39')'
      'AND MAT_ATIVO = '#39'S'#39
      'ORDER BY MAT_DESC')
    Left = 512
    Top = 384
  end
  object qrConsulta: TADOQuery
    Connection = FMenu.ADOConnection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT '
      #9'TR.TRANS_ID,'
      #9'TR.DATA, '
      #9'MT.MAT_DESC,'
      #9'IT.QTDE,'
      #9'IT.LOTE,'
      #9'MR.MARCA_DESC,'
      
        #9'(SELECT DESCRICAO FROM LOCAL WHERE LOCAL_ID = TR.ORIGEM) AS ORI' +
        'GEM,'
      
        #9'(SELECT DESCRICAO FROM LOCAL WHERE LOCAL_ID = TR.DESTINO) AS DE' +
        'STINO'
      'FROM'
      
        #9'TRANSFERENCIA TR INNER JOIN TRANS_ITEM IT ON TR.TRANS_ID = IT.T' +
        'RANS_ID'
      #9'INNER JOIN MATERIAIS MT ON IT.MAT_ID = MT.MAT_ID'
      #9'LEFT OUTER JOIN MARCAS MR ON IT.MARCA_ID = MR.MARCA_ID'
      'ORDER BY TR.TRANS_ID DESC, MT.MAT_DESC ASC')
    Left = 152
    Top = 432
