  object UPD_INV: TUpdateSQL
    ModifySQL.Strings = (
      'update INV'
      'set'
      '  Data = :Data,'
      '  N_Lote = :N_Lote,'
      '  Valid = :Valid,'
      '  Qtde_Inv = :Qtde_Inv,'
      '  Usuario = :Usuario,'
      '  Dt_Alt = :Dt_Alt,'
      '  Obs = :Obs,'
      '  Custo_Medio_Inv = :Custo_Medio_Inv'
      'where'
      '  Inv_ID = :OLD_Inv_ID')
    InsertSQL.Strings = (
      'insert into INV'
      
        '  (Data, N_Lote, Valid, Qtde_Inv, Usuario, Dt_Alt, Obs, Custo_Me' +
        'dio_Inv)'
      'values'
      
        '  (:Data, :N_Lote, :Valid, :Qtde_Inv, :Usuario, :Dt_Alt, :Obs, :' +
        'Custo_Medio_Inv)')
    DeleteSQL.Strings = (
      'delete from INV'
      'where'
      '  Inv_ID = :OLD_Inv_ID')
    Left = 698
    Top = 295
  end
