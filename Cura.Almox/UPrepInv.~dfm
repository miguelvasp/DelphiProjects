  object Q_INV: TwwQuery
    CachedUpdates = True
    BeforePost = Q_INVBeforePost
    AfterPost = Q_INVAfterPost
    AfterScroll = Q_INVAfterScroll
    OnCalcFields = Q_INVCalcFields
    DatabaseName = 'Cura'
    SessionName = 'Sec_BaseFin'
    RequestLive = True
    SQL.Strings = (
      'SELECT  I.*,'
      '        MAT.MAT_DESC, MAT.LocalArmazem, '
      #9'FORN.FOR_CODI,'
      #9'MAT.UNI_ID,'
      #9'M.MARCA_DESC,'
      #9'round(I.QTDE_INV,2) * ROUND(I.REPOSICAO,2) AS VALOR'
      ', MediaCompras3Meses as ConsumoTresMeses'
      'FROM    '#9'INV I,'
      #9'MATERIAIS MAT,'
      #9'CPAFORN FORN,'
      #9'MARCAS M'
      'WHERE   I.MAT_ID = MAT.MAT_ID'
      'AND     I.FOR_CODI = FORN.FOR_CODI'
      'AND      I.NUM_INV =(SELECT MAX(NUM_INV) NUM_INV FROM INV)'
      'AND'#9'M.MARCA_ID = I.MARCA_ID'
      'ORDER BY I.NUM_INV,'
