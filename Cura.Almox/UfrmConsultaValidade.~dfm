  object qrBusca: TADOQuery
    Connection = FMenu.ADOConnection
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'VALID'
        Attributes = [paNullable]
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 10
        Value = Null
      end>
    SQL.Strings = (
      ''
      'SELECT'#9
      
        'SUM(CASE WHEN ALM.QTDE IS NULL THEN 0.0 ELSE ALM.QTDE END) QTDE,' +
        ' '#9#9'MAT.MAT_ID, '#9#9
      'MAT.MAT_DESC,'#9#9
      'UNI.UNI_SIGLA, '#9#9#9
      'LOC.DESCRICAO LOCAL,                '
      'MAT.CUSTO_MEDIO, ALM.VALID, alm.n_lote'
      'FROM'#9#9
      'ALMOX'#9#9'ALM, '#9#9
      
        'MATERIAIS'#9'MAT INNER JOIN GRUPO_MATERIAL G ON MAT.GRU_ID = G.GRU_' +
        'ID '
      'LEFT OUTER JOIN UNIDADES UNI ON MAT.UNI_ID = UNI.UNI_ID, '#9#9
      'LOCAL'#9#9'LOC   '
      'WHERE'#9#9
      'ALM.MAT_ID = MAT.MAT_ID  '
      'AND'#9#9'ALM.LOCAL_ID'#9'= LOC.LOCAL_ID  '
      'AND'#9#9'ALM.TIPO = '#39'E'#39' '
      'AND'#9#9'ALM.QTDE_RETIRADA IS NULL  '
      'AND CONVERT(CHAR(10), ALM.VALID, 112) <= :VALID'
      'GROUP BY'#9
      'MAT.MAT_ID, '#9#9
      'MAT.MAT_DESC,'
      'LOC.DESCRICAO, '#9#9
      'UNI.UNI_SIGLA,              '
      'MAT.CUSTO_MEDIO, ALM.VALID, alm.n_lote'
      'ORDER BY MAT.MAT_DESC, LOC.DESCRICAO ')
    Left = 416
    Top = 176
    object qrBuscaMAT_DESC: TStringField
      DisplayLabel = 'Material'
      DisplayWidth = 40
      FieldName = 'MAT_DESC'
      Size = 60
    end
    object qrBuscaQTDE: TFloatField
