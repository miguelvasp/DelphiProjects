      #9#9'D.MARCA_DESC,'
      
        #9#9'CASE WHEN C.MAT_DESC IS not null THEN B.MAT_DESC ELSE C.MAT_DE' +
        'SC END AS MAT_DESC,'
      #9#9'B.STATUS ST_ITEM,'
      #9#9'B.UNI_COMP_ID,'
      #9#9'B.QTDE_COMP,'
      #9#9'B.QTDE_COMP_REC ,'
      #9#9'MM.MATMARCA_EMBALAGEM,'
      #9#9'B.ALTERAR,'
      #9#9'B.IORD_ID,'
      
        'CASE WHEN ((a.STATUS = '#39'A'#39' OR a.STATUS = '#39'P'#39') AND DATEADD(DAY, 1' +
        '0, DT_ENTREGA) <= GETDATE()  AND AutorizaOrdemVencida IS NULL) T' +
        'HEN '#39'Autorizar'#39' End as Autorizacao'
      'FROM    '#9#9'ORDEM_COMPRA A,'
      #9#9'ORDEM_COMPRAITEM B,'
      #9#9'MATERIAIS C,'
      #9#9'MARCAS D,'
      #9#9'MATERIAL_MARCA MM'
      'WHERE   '#9'A.ORD_ID = B.ORD_ID'
      'AND     '#9#9'B.MAT_ID = C.MAT_ID'
      'AND     '#9#9'B.MARCA_ID = D.MARCA_ID'
      'AND'#9#9'MM.MATMARCA_ID = B.MATMARCA_ID'
      'ORDER BY '#9'A.ORD_ID DESC')
    UpdateObject = UPD_OC
    ControlType.Strings = (
      'STATUS;CustomEdit;wwDBComboBox1'
      'ST_ITEM;CustomEdit;cboStPesq;F'
      'ALTERAR;CheckBox;S;')
    ValidateWithMask = True
    Left = 148
    Top = 312
