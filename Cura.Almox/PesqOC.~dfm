  object UPD_OC: TUpdateSQL
    ModifySQL.Strings = (
      'update ORDEM_COMPRAITEM'
      'set'
      '  ALTERAR = :ALTERAR'
      'where'
      '  IORD_ID = :OLD_IORD_ID')
    InsertSQL.Strings = (
      'insert into ORDEM_COMPRAITEM'
      '  (ALTERAR)'
      'values'
      '  (:ALTERAR)')
    DeleteSQL.Strings = (
      'delete from ORDEM_COMPRAITEM'
      'where'
      '  IORD_ID = :OLD_IORD_ID')
