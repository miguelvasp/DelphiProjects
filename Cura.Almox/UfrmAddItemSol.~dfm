  object qrMatCadastro: TADOQuery
    Connection = FMenu.ADOConnection
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      ''
      'SELECT MAT_ID, MAT_DESC, uni_id ,'
      '('
      'SELECT'#9
      
        #9#9#9'CASE WHEN SUM(ALM.QTDE) IS NULL  THEN 0 ELSE SUM(ALM.QTDE) EN' +
        'D '#9#9
      #9#9'FROM'
      #9#9#9'ALMOX'#9#9'ALM, '#9#9
      #9#9#9'MATERIAIS'#9'MAT, '#9#9
      #9#9#9'LOCAL'#9#9'LOC   '
      #9#9'WHERE'#9#9
      #9#9#9'ALM.MAT_ID = MAT.MAT_ID  '
      #9#9#9'AND'#9'ALM.LOCAL_ID = LOC.LOCAL_ID  '
      #9#9#9'AND'#9'ALM.TIPO = '#39'E'#39' '
      #9#9#9'AND'#9'ALM.QTDE_RETIRADA IS NULL  '
      #9#9#9'AND MAT.MAT_ID = A.MAT_ID'
      ') AS ESTOQUE'
      'FROM MATERIAIS A'
      'WHERE GENERICO NOT IN ('#39'S'#39')'
      'AND MAT_ATIVO = '#39'S'#39
      'ORDER BY MAT_DESC')
    Left = 464
    Top = 16
    object StringField1: TStringField
      DisplayLabel = 'Material'
      DisplayWidth = 25
      FieldName = 'MAT_DESC'
      Size = 60
    end
    object qrMatCadastroESTOQUE: TFloatField
      DisplayLabel = 'Estoque'
      DisplayWidth = 5
      FieldName = 'ESTOQUE'
      ReadOnly = True
    end
    object AutoIncField1: TAutoIncField
      DisplayWidth = 10
      FieldName = 'MAT_ID'
      ReadOnly = True
    end
    object qrMatCadastrouni_id: TIntegerField
      DisplayWidth = 10
      FieldName = 'uni_id'
      Visible = False
    end
  end
