  object qrListagem: TADOQuery
    Connection = FMenu.ADOConnection
    Parameters = <>
    SQL.Strings = (
      
        ' SELECT distinct REC.*,  convert( varchar, ( SELECT DISTINCT Ven' +
        'c = STUFF(( SELECT '#39' '#39' + CONVERT(VARCHAR,Dt_Vcto, 103 ) FROM Rec' +
        '_Vcto  WHERE REC_ID = REC.Rec_ID  FOR XML PATH ('#39#39')), 1, 1, '#39#39') ' +
        ' FROM Rec_Vcto  WHERE REC_ID = REC.Rec_ID )) AS Venc FROM Rec_NF' +
        ' REC  INNER JOIN Rec_Item ITEM ON REC.Rec_ID = ITEM.Rec_ID WHERE' +
        ' 1 = 1  AND    REC.DT_ENTRADA  between '#9'  Convert(SMALLDATETIME,' +
        #39'20200101'#39') AND Convert(SMALLDATETIME,'#39'20200130'#39') ORDER BY  REC.' +
        'NFIS, REC.DT_ENTRADA DESC ')
    Left = 168
    Top = 360
