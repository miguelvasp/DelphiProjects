  object Q_MARCA: TwwQuery
    DatabaseName = 'CURA'
    SessionName = 'Sec_BaseFin'
    DataSource = DS_MAT
    SQL.Strings = (
      'SELECT  M.MARCA_ID,'
      #9'M.MARCA_DESC'
      'FROM     MARCAS  M,'
      #9'MATERIAL_MARCA MM'
      'WHERE'#9'MM.MARCA_ID = M.MARCA_ID'
      'AND'#9'MM.MAT_ID = :MAT_ID'
      'ORDER  BY MARCA_DESC')
    ValidateWithMask = True
    Left = 626
    Top = 216
