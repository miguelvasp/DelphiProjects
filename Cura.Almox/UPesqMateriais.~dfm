  object qrCodigosXML: TADOQuery
    Connection = FMenu.ADOConnection
    Parameters = <>
    SQL.Strings = (
      'SELECT *'
      'FROM ('
      'SELECT MAT_DESC, CodigoFornecedor, '#39#39' Fornecedor, '#39#39' EAN '
      'FROM MATERIAIS '
      
        'WHERE CodigoFornecedor IS NOT NULL AND LTRIM(RTRIM(CodigoFornece' +
        'dor)) <> '#39#39
      ''
      'UNION ALL'
      ''
      'SELECT MAT_DESC, Codigo, FOR_CODI, x.EAN '
      'FROM MATERIAIS M '
      'INNER JOIN MateriaisCodigoXML X ON M.MAT_ID = X.MAT_ID'
      ')AS A'
      'ORDER BY 2')
    Left = 264
    Top = 216
    object qrCodigosXMLMAT_DESC: TStringField
      FieldName = 'MAT_DESC'
      ReadOnly = True
      Size = 100
    end
    object qrCodigosXMLCodigoFornecedor: TStringField
      FieldName = 'CodigoFornecedor'
      ReadOnly = True
      Size = 255
    end
    object qrCodigosXMLFornecedor: TStringField
      FieldName = 'Fornecedor'
      ReadOnly = True
      Size = 50
    end
    object qrCodigosXMLEAN: TStringField
      FieldName = 'EAN'
      ReadOnly = True
      Size = 50
    end
  end
