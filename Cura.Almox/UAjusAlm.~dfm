  object Q_SBOBAJ: TwwQuery
    CachedUpdates = True
    DatabaseName = 'Cura'
    SessionName = 'Sec_BaseFin'
    SQL.Strings = (
      
        'SELECT'#9#9'CASE WHEN ESTQ.QTDE IS NULL THEN 0 ELSE ESTQ.QTDE END AS' +
        ' QTDE,'
      #9#9'CASE WHEN ESTQ.PESO IS NULL THEN 0 ELSE ESTQ.PESO END AS PESO'
      'FROM'#9#9'APONT_ESTOQUE '#9'ESTQ,'
      #9#9'PRODUTOS PROD,'
      #9#9'PRODUTO_TIPO PT'
      'WHERE'#9#9'ESTQ.RG_BOBINA = :RG_BOBINA'
      'AND'#9#9'ESTQ.PROD_ID = PROD.PROD_ID'
      'AND'#9#9'PROD.TP_PROD_ID = PT.TP_PROD_ID'
      ' '
      'AND'#9#9'ESTQ.TIPO_ESTOQUE = '#39'E'#39
      'AND'#9#9'ESTQ.QTDE_RETIRADA IS NULL'
      ''
      ''
      ''
      ''
      ''
      ''
      ''
      '')
    ValidateWithMask = True
