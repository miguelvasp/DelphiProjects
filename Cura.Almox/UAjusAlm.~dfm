  object UPD_AJALM: TUpdateSQL
    ModifySQL.Strings = (
      'update ALMOX'
      'set'
      '  Almox_ID_Mestre = :Almox_ID_Mestre,'
      '  For_Codi = :For_Codi,'
      '  Local_ID = :Local_ID,'
      '  Marca_ID = :Marca_ID,'
      '  N_Lote = :N_Lote,'
      '  Valid = :Valid,'
      '  Qtde = :Qtde,'
      '  Qtde_Retirada = :Qtde_Retirada,'
      '  Qtde_Input = :Qtde_Input,'
      '  Tipo = :Tipo,'
      '  Uni_Sigla = :Uni_Sigla,'
      '  Obs = :Obs,'
      '  Data = :Data,'
      '  Usuario = :Usuario,'
      '  TD_ID = :TD_ID,'
