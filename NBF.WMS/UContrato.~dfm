  object UPD_CTTxESPEC: TUpdateSQL
    ModifySQL.Strings = (
      'update CONTRATO_ESPECIFICACOES'
      'set'
      '  CTTESPEC_ORDEM = :CTTESPEC_ORDEM,'
      '  CTT_ID = :CTT_ID,'
      '  SER_ID = :SER_ID,'
      '  TSER_ID = :TSER_ID,'
      '  CTTESPEC_FATOR = :CTTESPEC_FATOR,'
      '  CTTESPEC_MINIMA = :CTTESPEC_MINIMA,'
      '  UCOB_ID = :UCOB_ID,'
      '  CTTESPEC_TPCARGA = :CTTESPEC_TPCARGA,'
      '  CLIF_ID = :CLIF_ID,'
      '  VEIC_ID = :VEIC_ID,'
      '  REG_ID = :REG_ID,'
      '  TPRO_ID = :TPRO_ID,'
      '  CTTESPEC_PESO_DE = :CTTESPEC_PESO_DE,'
      '  CTTESPEC_PESO_ATE = :CTTESPEC_PESO_ATE,'
      '  CTTESPEC_AREA_DE = :CTTESPEC_AREA_DE,'
      '  CTTESPEC_AREA_ATE = :CTTESPEC_AREA_ATE,'
      '  CTTESPEC_QTDEPALLET_DE = :CTTESPEC_QTDEPALLET_DE,'
      '  CTTESPEC_QTDEPALLET_ATE = :CTTESPEC_QTDEPALLET_ATE,'
      '  CTTESPEC_VOL_DE = :CTTESPEC_VOL_DE,'
      '  CTTESPEC_VOL_ATE = :CTTESPEC_VOL_ATE,'
      '  CTTSERV_ID = :CTTSERV_ID'
      'where'
      '  CTTESPEC_ID = :OLD_CTTESPEC_ID')
    InsertSQL.Strings = (
      'insert into CONTRATO_ESPECIFICACOES'
      '  (CTTESPEC_ORDEM, CTT_ID, SER_ID, TSER_ID, CTTESPEC_FATOR, '
      
        '   CTTESPEC_MINIMA, UCOB_ID, CTTESPEC_TPCARGA, CLIF_ID, VEIC_ID,' +
        ' '
      'REG_ID, '
      '   TPRO_ID, CTTESPEC_PESO_DE, CTTESPEC_PESO_ATE, '
      'CTTESPEC_AREA_DE, CTTESPEC_AREA_ATE, '
      '   CTTESPEC_QTDEPALLET_DE, CTTESPEC_QTDEPALLET_ATE, '
      'CTTESPEC_VOL_DE, CTTESPEC_VOL_ATE, CTTSERV_ID)'
      'values'
      
        '  (:CTTESPEC_ORDEM, :CTT_ID, :SER_ID, :TSER_ID, :CTTESPEC_FATOR,' +
        ' '
      
        '   :CTTESPEC_MINIMA, :UCOB_ID, :CTTESPEC_TPCARGA, :CLIF_ID, :VEI' +
        'C_ID, '
      ':REG_ID, '
      '   :TPRO_ID, :CTTESPEC_PESO_DE, :CTTESPEC_PESO_ATE, '
      ':CTTESPEC_AREA_DE, '
      '   :CTTESPEC_AREA_ATE, :CTTESPEC_QTDEPALLET_DE, '
      ':CTTESPEC_QTDEPALLET_ATE, '
      '   :CTTESPEC_VOL_DE, :CTTESPEC_VOL_ATE, :CTTSERV_ID)')
    DeleteSQL.Strings = (
      'delete from CONTRATO_ESPECIFICACOES'
      'where'
      '  CTTESPEC_ID = :OLD_CTTESPEC_ID')
    Left = 703
    Top = 352
  end
