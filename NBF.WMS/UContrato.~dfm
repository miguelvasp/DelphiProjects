  object UPD_CTT: TUpdateSQL
    ModifySQL.Strings = (
      'update CONTRATO'
      'set'
      '  CLIN_ID = :CLIN_ID,'
      '  CTT_NOME = :CTT_NOME,'
      '  TOS_ID = :TOS_ID,'
      '  CTT_DT_INICIO = :CTT_DT_INICIO,'
      '  CTT_DT_FINAL = :CTT_DT_FINAL,'
      '  EMP_ID = :EMP_ID,'
      '  CTT_TERCEIRO = :CTT_TERCEIRO,'
      '  PAR_ID = :PAR_ID,'
      '  MIN_M2 = :MIN_M2'
      'where'
      '  CTT_ID = :OLD_CTT_ID')
    InsertSQL.Strings = (
      'insert into CONTRATO'
      
        '  (CLIN_ID, CTT_NOME, TOS_ID, CTT_DT_INICIO, CTT_DT_FINAL, EMP_I' +
        'D, CTT_TERCEIRO, '
      '   PAR_ID, MIN_M2)'
      'values'
      
        '  (:CLIN_ID, :CTT_NOME, :TOS_ID, :CTT_DT_INICIO, :CTT_DT_FINAL, ' +
        ':EMP_ID, '
      '   :CTT_TERCEIRO, :PAR_ID, :MIN_M2)')
    DeleteSQL.Strings = (
      'delete from CONTRATO'
      'where'
      '  CTT_ID = :OLD_CTT_ID')
    Left = 540
    Top = 192
  end
