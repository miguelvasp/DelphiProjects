{$R *.DFM}

procedure TfrmContratoFreteRel.FormCreate(Sender: TObject);
begin
  qrPAram.Open;
  qrIbge.Parameters[0].Value := qrPAramIBGE_ID.AsInteger;
  qrIbge.Open;

  lbData.Caption := 'Jaboatão dos Guararapes, ' + formatdatetime('dd "de" mmmm "de" yyyy', frmContratoFreteCad.qrContratoData.AsDateTime);
end;

end.
