      'update EMP_TEMP'
      'set'
      '  USUARIO = :USUARIO,'
      '  FIL_CODI = :FIL_CODI,'
      '  SEL = :SEL'
      'where'
      '  id = :OLD_id')
    InsertSQL.Strings = (
      'insert into EMP_TEMP'
      '  (USUARIO, FIL_CODI, SEL)'
      'values'
      '  (:USUARIO, :FIL_CODI, :SEL)')
    DeleteSQL.Strings = (
      'delete from EMP_TEMP'
      'where'
      '  id = :OLD_id')
