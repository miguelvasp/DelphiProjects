  object Q_CHEQ: TwwQuery
    DatabaseName = 'Financeiro'
    Filtered = True
    SessionName = 'Sec_BaseFin'
    SQL.Strings = (
      'SELECT A.NUMERO,'
      'A.CHEQUENUMERO,'
      'A.DATAEMISSAO,'
      'B.FOR_RAZA,'
      'A.NOME,'
      'A.VALOR,'
      'C.BAN_NOME,'
      'A.TIPO,'
      'CASE A.EMITIDO'
      #9'WHEN '#39'N'#39' THEN '#39'A Emitir'#39' '
      '        '#9'WHEN '#39'S'#39' THEN '#39'Emitido'#39
      '        '#9'WHEN '#39'C'#39' THEN '#39'Cancelado'#39' END AS EMITIDO,'
      'A.MARCADOR'
      'FROM CPACHEQ A'
      'LEFT OUTER JOIN CPAFORN B ON A.FAVORECIDO = B.FOR_CODI'
      'LEFT OUTER JOIN CPABANC C ON A.BANCO = C.BAN_CODI')
    ValidateWithMask = True
    Left = 186
    Top = 224
