      '  MOT_TEL_PESS = :MOT_TEL_PESS,'
      '  MOT_TEL_COM = :MOT_TEL_COM,'
      '  MOT_PAMCARD = :MOT_PAMCARD,'
      '  MOT_PAMCARD_VIG = :MOT_PAMCARD_VIG,'
      '  MOT_ENDERECO_COMPL = :MOT_ENDERECO_COMPL,'
      '  MOT_ENDERECO_BAIRRO = :MOT_ENDERECO_BAIRRO,'
      '  VEIC_ID = :VEIC_ID,'
      '  MOT_VEIC_PLACA = :MOT_VEIC_PLACA,'
      '  MOT_VEIC_CIDADE = :MOT_VEIC_CIDADE,'
      '  MOT_VEIC_UF_SIGLA = :MOT_VEIC_UF_SIGLA,'
      '  MOT_VEIC_MARCA = :MOT_VEIC_MARCA,'
      '  MOT_VEIC_ANO = :MOT_VEIC_ANO,'
      '  MOT_VEIC_MODELO = :MOT_VEIC_MODELO,'
      '  MOT_VEIC_COR = :MOT_VEIC_COR,'
      '  MOT_VEIC_CHASSI = :MOT_VEIC_CHASSI,'
      '  MOT_CARRETA_PLACA = :MOT_CARRETA_PLACA,'
      '  MOT_CARRETA_CIDADE = :MOT_CARRETA_CIDADE,'
      '  MOT_CARRETA_UF_SIGLA = :MOT_CARRETA_UF_SIGLA,'
      '  MOT_CARRETA_MARCA = :MOT_CARRETA_MARCA,'
      '  MOT_CARRETA_ANO = :MOT_CARRETA_ANO,'
      '  MOT_CARRETA_MODELO = :MOT_CARRETA_MODELO,'
      '  MOT_CARRETA_COR = :MOT_CARRETA_COR,'
      '  MOT_CARRETA_CHASSI = :MOT_CARRETA_CHASSI,'
      '  MOT_PAMC_NUM = :MOT_PAMC_NUM,'
      '  MOT_NUMPIS = :MOT_NUMPIS,'
      '  MOT_CARRETA_COMPR = :MOT_CARRETA_COMPR,'
      '  MOT_CARRETA_LARG = :MOT_CARRETA_LARG,'
      '  MOT_CARRETA_ALT = :MOT_CARRETA_ALT,'
      '  MOT_CARRETA_M3 = :MOT_CARRETA_M3,'
      '   MOT_NEXTEL = :MOT_NEXTEL,'
      '  MOT_EQUIP = :MOT_EQUIP,'
      '  RENAVAN = :RENAVAN,'
      '  PADRAO = :PADRAO,'
      'CIOT = :CIOT'
      'where'
      '  MOT_ID = :OLD_MOT_ID')
    InsertSQL.Strings = (
      'insert into TRANSPORTADORA_MOTORISTA'
      '  (TRANS_ID, MOT_NOME, MOT_ENDERECO, MOT_MUNICIPIO, MOT_CEP, '
      'UF_SIGLA, '
      '   MOT_CPF, MOT_RG, MOT_DT_NASC, MOT_RG_ORGAO, MOT_CNH, '
      'MOT_CNH_CATEG, '
      '   MOT_CNH_VALID, MOT_PAI, MOT_MAE, MOT_TEL_PESS, MOT_TEL_COM, '
      'MOT_PAMCARD, '
      '   MOT_PAMCARD_VIG, MOT_ENDERECO_COMPL, MOT_ENDERECO_BAIRRO, '
      'VEIC_ID, MOT_VEIC_PLACA, '
      '   MOT_VEIC_CIDADE, MOT_VEIC_UF_SIGLA, MOT_VEIC_MARCA, '
      'MOT_VEIC_ANO, MOT_VEIC_MODELO, '
      '   MOT_VEIC_COR, MOT_VEIC_CHASSI, MOT_CARRETA_PLACA, '
      'MOT_CARRETA_CIDADE, '
      '   MOT_CARRETA_UF_SIGLA, MOT_CARRETA_MARCA, MOT_CARRETA_ANO, '
      'MOT_CARRETA_MODELO, '
      '   MOT_CARRETA_COR, MOT_CARRETA_CHASSI, MOT_PAMC_NUM, '
      'MOT_NUMPIS, MOT_CARRETA_COMPR, '
      '   MOT_CARRETA_LARG, MOT_CARRETA_ALT, MOT_CARRETA_M3, '
      'MOT_NEXTEL, MOT_EQUIP, RENAVAN, PADRAO, CIOT)'
      'values'
      
        '  (:TRANS_ID, :MOT_NOME, :MOT_ENDERECO, :MOT_MUNICIPIO, :MOT_CEP' +
        ', '
      ':UF_SIGLA, '
      '   :MOT_CPF, :MOT_RG, :MOT_DT_NASC, :MOT_RG_ORGAO, :MOT_CNH, '
      ':MOT_CNH_CATEG, '
      
        '   :MOT_CNH_VALID, :MOT_PAI, :MOT_MAE, :MOT_TEL_PESS, :MOT_TEL_C' +
        'OM, '
      ':MOT_PAMCARD, '
      '   :MOT_PAMCARD_VIG, :MOT_ENDERECO_COMPL, :MOT_ENDERECO_BAIRRO, '
      ':VEIC_ID, '
      '   :MOT_VEIC_PLACA, :MOT_VEIC_CIDADE, :MOT_VEIC_UF_SIGLA, '
      ':MOT_VEIC_MARCA, '
      '   :MOT_VEIC_ANO, :MOT_VEIC_MODELO, :MOT_VEIC_COR, '
      ':MOT_VEIC_CHASSI, :MOT_CARRETA_PLACA, '
      '   :MOT_CARRETA_CIDADE, :MOT_CARRETA_UF_SIGLA, '
      ':MOT_CARRETA_MARCA, :MOT_CARRETA_ANO, '
      '   :MOT_CARRETA_MODELO, :MOT_CARRETA_COR, :MOT_CARRETA_CHASSI, '
      ':MOT_PAMC_NUM, '
      '   :MOT_NUMPIS, :MOT_CARRETA_COMPR, :MOT_CARRETA_LARG, '
      ':MOT_CARRETA_ALT, '
      '   :MOT_CARRETA_M3, :MOT_NEXTEL, :MOT_EQUIP, :RENAVAN, :PADRAO, '
      ':CIOT)')
    DeleteSQL.Strings = (
      'delete from TRANSPORTADORA_MOTORISTA'
      'where'
      '  MOT_ID = :OLD_MOT_ID')
    Left = 307
    Top = 64
  end
