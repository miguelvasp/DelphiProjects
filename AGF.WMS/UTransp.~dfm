      '  TRANS_FAX = :TRANS_FAX,'
      '  TRANS_EMAIL = :TRANS_EMAIL,'
      '  TRANS_CONTATO = :TRANS_CONTATO,'
      '  TRANS_FLAG = :TRANS_FLAG,'
      '  CON_CODI = :CON_CODI,'
      '  MUN_ID = :MUN_ID,'
      '  TRANS_PESSOA = :TRANS_PESSOA,'
      '  TRANS_REDES = :TRANS_REDES,'
      '  PIS = :PIS,'
      '  RNTRC = :RNTRC,'
      'TRANS_CATEGORIA = :TRANS_CATEGORIA,'
      'AGENCIA = :AGENCIA,'
      'BANCO = :BANCO,'
      'CONTACORRENTE = :CONTACORRENTE,'
      'TIPOCONTA = :TIPOCONTA'
      'where'
      '  TRANS_ID = :OLD_TRANS_ID')
    InsertSQL.Strings = (
      'insert into TRANSPORTADORA'
      '  (TRANS_FANTASIA, TRANS_RAZA, TRANS_CNPJ, TRANS_INSCRICAO, '
      'TRANS_ENDERECO, '
