      'set'
      '  TRANS_ID = :TRANS_ID,'
      '  MOT_NOME = :MOT_NOME,'
      '  MOT_ENDERECO = :MOT_ENDERECO,'
      '  MOT_MUNICIPIO = :MOT_MUNICIPIO,'
      '  MOT_CEP = :MOT_CEP,'
