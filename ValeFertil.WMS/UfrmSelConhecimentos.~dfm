  object qrOCE: TADOQuery
    Connection = FMenu.ADOConnection
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'OS_ID'
        Attributes = [paSigned, paNullable]
        DataType = ftInteger
        Precision = 10
        Size = 4
        Value = Null
      end>
    SQL.Strings = (
      'SELECT '
      'A.SEL, ORD_ID, A.OS_ID,'
      'A.ORD_DATA,'
      'A.ORD_CONH,'
      'A.ORD_NOTAS,'
      'B.CLIN_RAZA,'
      'C.CLIF_RAZA , '
      'A.ORD_VLR_FRETE, '
      'A.ORD_VLR_OUTROS, '
      'A.ORD_VLR_ICMS, '
      'A.ORD_TOTALPREST'
      
        'FROM ORDEM_COLETA_ENTREGA A  ,CLIENTENBF B,  CLIENTEFINAL C , OS' +
        ' D'
      'WHERE A.CLIN_ID = B.CLIN_ID'
      'AND  A.CLIF_ID = C.CLIF_ID'
      'AND A.OS_ID = D.OS_ID'
      'AND A.OS_ID = :OS_ID'
      'AND A.ORD_STATUS = '#39'E'#39
      'AND A.FAT_ID IS NULL'
      'ORDER BY A.ORD_CONH')
    Left = 160
    Top = 208
